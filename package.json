{
    "name": "kigc_discordbot",
    "version": "1.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "main": "build/main.js",
    "scripts": {
        "build": "tsc",
        "dev": "npm run build && npm start",
        "start": "node --experimental-loader=extensionless build/main.js",
        "watch": "nodemon --exec npm run dev --watch src --ext ts"
    },
    "dependencies": {
        "@discordx/importer": "^1.3.1",
        "@discordx/pagination": "^3.5.4",
        "discord.js": "^14.15.3",
        "discordx": "^11.11.1",
        "dotenv": "^16.4.5",
        "eslint-plugin-prettier": "^5.2.1",
        "extensionless": "^1.9.9",
        "reflect-metadata": "^0.2.2",
        "tsyringe": "^4.8.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.0",
        "@dareharu/biome-config": "^5.0.1",
        "@eslint/js": "^9.10.0",
        "@types/node": "^20.14.4",
        "eslint": "^9.10.0",
        "globals": "^15.9.0",
        "nodemon": "^3.1.3",
        "prettier": "^3.3.2",
        "ts-node": "^10.9.2",
        "typescript": "5.4.5",
        "typescript-eslint": "^8.5.0"
    },
    "extensionless": {
        "lookFor": [
            "js",
            "mjs",
            "cjs"
        ]
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    }
}
